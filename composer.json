{
    "name": "webikevn/remote-ssh",
    "description": "Remote SSH access for The Laravel Framework.",
    "license": "MIT",
    "authors": [
        {
            "name": "Nguyen Giang",
            "email": "nguyen.giang@rivercrane.com.vn"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^6.0|^7.0",
        "illuminate/filesystem": "^6.0|^7.0",
        "phpseclib/phpseclib": "^2.0"
    },
    "require-dev": {
        "illuminate/console": "^6.0|^7.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0-dev"
        },
        "laravel": {
            "providers": [
                "Webike\\Remote\\RemoteServiceProvider"
            ],
            "aliases": {
                "SSH": "Webike\\Remote\\RemoteFacade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Webike\\Remote\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
